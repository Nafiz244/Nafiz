
public class Alphabet {
	  public static void main(String[] args) 
	    {
	       int rows = 5;
	         
	        System.out.println("Here is your pattern....!!!");
	         
	        for (int i = rows; i >= 1; i--) 
	        {
	            for (int j = i; j >= 1; j--)
	            {
	                System.out.print(j+" ");
	            }
	             
	            System.out.println();
	        }
	         
	    }
	}

}
